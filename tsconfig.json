{
  "compilerOptions": {
    "types": [
        "meteor-typings",
        "node"
    ],
    "sourceMap": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "noImplicitAny": false,
    "module": "commonjs",
    "target": "es5",
    "lib": ["es2015", "dom"],
    "jsx": "react",
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "*": ["*"]
    }
  }
}