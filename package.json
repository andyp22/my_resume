{
  "name": "meteor_boilerplate",
  "version": "0.0.1",
  "description": "A simple MeteorJS project boilerplate.",
  "main": "main.js",
  "scripts": {
    "start": "meteor run",
    "lint": "eslint .",
    "pretest": "npm run lint --silent"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andyp22/meteor_boilerplate.git"
  },
  "keywords": [
    "meteor",
    "boilerplate"
  ],
  "author": "Andrew Page",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andyp22/meteor_boilerplate/issues"
  },
  "homepage": "https://github.com/andyp22/meteor_boilerplate",
  "dependencies": {
    "meteor-node-stubs": "~0.2.0",
    "skrollr": "0.6.26",
    "imagesloaded": "^4.1.0",
    "cleverbot.io": "^1.0.4",
    "fibers": "^1.0.10",
    "typed.js": "^1.1.1",
    "winston": "^2.2.0",
    "winston-loggly": "^1.3.1"
  },
  "devDependencies": {
    "eslint": "2.8.0",
    "eslint-config-airbnb": "7.0.0",
    "eslint-plugin-meteor": "3.5.2"
  },
  "peerDependencies": {
    "eslint": ">=0.8.0"
  },
  "eslintConfig": {
    "plugins": [
      "meteor"
